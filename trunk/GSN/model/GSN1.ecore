        eType="#//ElementStatus"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="isPartOf" lowerBound="1"
        eType="#//ArgumentModule" eOpposite="#//ArgumentModule/containsElement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SpinalElement" abstract="true" eSuperTypes="#//ArgumentElement">
