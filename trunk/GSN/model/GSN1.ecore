    <eLiterals name="ToBeInstantiated" value="2"/>
    <eLiterals name="ToBeDevelopedAndInstantiated" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ElementVisibility">
